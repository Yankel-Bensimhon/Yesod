{
  "name": "yesod-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "setup": "./setup.sh",
    "quick-start": "./quick-start.sh",
    "validate-env": "node validate-env.js",
    "postinstall": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:integration": "jest --testMatch='**/*.integration.test.ts'",
    "test:e2e": "playwright test",
    "test:smoke": "jest --testMatch='**/*.smoke.test.ts'",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true npm run build",
    "bundle-analyzer": "cross-env ANALYZE=true npm run build",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:backup": "node scripts/backup-database.js",
    "health-check:production": "node scripts/health-check.js",
    "performance:lighthouse": "lighthouse-ci autorun",
    "performance:load-test": "node scripts/load-test.js",
    "sentry:create-release": "sentry-cli releases new $GITHUB_SHA",
    "sentry:associate-commits": "sentry-cli releases set-commits $GITHUB_SHA --auto",
    "metrics:business-check": "node scripts/business-metrics.js",
    "metrics:user-engagement": "node scripts/engagement-metrics.js",
    "compliance:rgpd-check": "node scripts/rgpd-compliance.js",
    "compliance:data-privacy-audit": "node scripts/privacy-audit.js",
    "generate:compliance-report": "node scripts/compliance-report.js",
    "monitor:health-check": "node scripts/monitor-health.js",
    "monitor:performance-check": "node scripts/monitor-performance.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@sentry/nextjs": "^9.42.1",
    "@sentry/node": "^9.42.1",
    "@sentry/tracing": "^7.120.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/ioredis": "^5.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/pdfkit": "^0.17.2",
    "@types/redis": "^4.0.11",
    "@upstash/redis": "^1.35.2",
    "bullmq": "^5.56.8",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.11",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.526.0",
    "next": "15.4.4",
    "next-auth": "^4.24.11",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.1",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "recharts": "^3.1.0",
    "redis": "^5.6.1",
    "tailwind-merge": "^3.3.1",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "bcryptjs": "^3.0.2",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "lighthouse-ci": "^1.13.1",
    "prisma": "^6.12.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "typescript": "^5"
  }
}
